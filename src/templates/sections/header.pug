include ../_mixins.pug

- 
  const nav = [
    ['Home', '/'],
    ['Book time', '/booking'],
    ['Contact us', '/contact-us'],
  ]

- 
  const langs = [
    ['English', '#'],
    ['Dubai', '#'],
    ['German', '#'],
    ['Israel', '#'],
  ]

header.header
    .header__top
        nav.nav.header__nav
            +nav('nav', nav)
        .langs.header__langs
            +langs('langs', langs)
    .header__bottom
        object(type="image/svg+xml" data=require('images/logo.svg').default).header__logo
